# Routes
# ~~~~
->      /common                     common.Routes

# Main index page
GET     /                                     controllers.EntryPointController.index

GET     /trade-type                           controllers.TradeTypeController.renderForm

POST    /select-trade-type                    controllers.TradeTypeController.handleSubmit

POST    /export-category/select               controllers.categories.ExportCategoryController.handleSubmit

POST    /export-category/arts-cultural        controllers.categories.ArtsCulturalController.handleSubmit

POST    /select-goods-type                    controllers.GoodsTypeController.handleSubmit

POST    /export-search-physical-goods         controllers.search.PhysicalGoodsSearchController.handleSubmit

POST    /export-search-results-physical-goods controllers.search.PhysicalGoodsSearchResultsController.handleSubmit

POST    /control-code                         controllers.ControlCodeController.handleSubmit

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                         controllers.Assets.versioned(path="/public", file: Asset)
